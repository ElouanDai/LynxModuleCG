{
  "name": "@byted-crystal/dyhuodong-magic-related-users",
  "version": "2.1.2",
  "description": "魔方用户相关信息",
  "license": "ISC",
  "maintainers": [
    "mashichen.msc"
  ],
  "exports": {
    ".": "./src/components/index/index",
    "./dist/*": "./dist/components/index/*",
    "./lepus": "./src/components/index/utils/index.lepus.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "exit 0",
    "build:demo": "lina build --demo",
    "build:h5": "CUSTOM_BUILD_DSL=Lynx2web lina build -o ./dist/web",
    "prepublish:only": "lcb build -s src/components -o dist/components && npm run build:h5",
    "start:demo": "lina start --demo",
    "start:h5": "CUSTOM_BUILD_DSL=Lynx2web lina start",
    "test": "exit 0 && npm run build:demo && jest",
    "test:type": "tsc --noemit",
    "test:update": "npm run build:demo && jest -u"
  },
  "dependencies": {
    "@byted-so-design/user-cell": "2.3.1"
  },
  "devDependencies": {
    "@byted-lina/cli": "workspace:*",
    "@byted-lina/component-build": "workspace:*",
    "@byted-lina/crystal-scripts": "workspace:*",
    "@byted-lina/lepus-utils": "workspace:*",
    "@byted-lina/lina-config-douyin-search": "workspace:*",
    "@byted-lina/lina-douyin-search-mixins": "workspace:*",
    "@byted-lina/lina-mixin-preserve-data": "workspace:*",
    "@byted-lina/morphling-bridge": "workspace:*",
    "@byted-lina/morphling-icons": "workspace:*",
    "@byted-lina/morphling-types": "workspace:*",
    "@byted-lina/runtime-ng": "workspace:*",
    "@byted-lina/shared-configs": "workspace:*",
    "@byted-lina/types": "workspace:*",
    "@byted-lina/utils": "workspace:*",
    "@byted-lynx/testing": "alpha",
    "@byted-lynx/testing-utils": "^0.2.2",
    "@ies/aweme-color-theme": "^0.1.21",
    "@types/jest": "^27.0.1",
    "@types/node": "^18.11.17",
    "jest": "^27.4.5",
    "tailwindcss": "2.2.19",
    "ts-jest": "^27.1.2",
    "tslib": "^2.4.0",
    "typescript": "^4.5.2"
  }
}