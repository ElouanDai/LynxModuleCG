{
  "name": "@byted/eslint-plugin-eden",
  "version": "3.1.6",
  "description": "Eden's base JS ESLint plugin",
  "main": "index.js",
  "keywords": [
    "eslint",
    "eden",
    "javascript",
    "styleguide",
    "es2015",
    "es2016",
    "es2017",
    "es2018"
  ],
  "peerDependencies": {
    "eslint": "*"
  },
  "engines": {
    "node": ">= 4"
  },
  "license": "MIT",
  "dependencies": {
    "@types/node": "^14.0.14",
    "eslint-plugin-import": "^2.26.0"
  },
  "eden-mono": {
    "packagePublish": {
      "alias": {
        "name": "@ies/eslint-plugin-eden",
        "markAsDeprecated": false
      }
    }
  },
  "scripts": {
    "prelint": "editorconfig-tools check * rules/* test/*",
    "lint": "eslint --report-unused-disable-directives .",
    "pretravis": ":",
    "posttravis": ":"
  }
}