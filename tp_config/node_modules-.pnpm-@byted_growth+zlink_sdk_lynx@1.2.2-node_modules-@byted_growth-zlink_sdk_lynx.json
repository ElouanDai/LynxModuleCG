{
  "name": "@byted-growth/zlink-sdk-lynx",
  "version": "1.2.2",
  "types": "./dist/types/index.d.ts",
  "jsnext:source": "./src/index.ts",
  "main": "./dist/lib/index.js",
  "module": "./dist/es/index.js",
  "scripts": {
    "prepare": "edenx build && husky install",
    "dev": "edenx dev",
    "build": "edenx build",
    "build:watch": "edenx build -w",
    "reset": "npx rimraf ./**/node_modules",
    "lint": "edenx lint",
    "lint:error": "edenx lint --quiet",
    "change": "edenx change",
    "bump": "edenx bump",
    "pre": "edenx pre",
    "release": "edenx release",
    "gen-release-note": "edenx gen-release-note",
    "new": "edenx new",
    "upgrade": "edenx upgrade"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ],
    "*.{js,jsx,mjs,mjsx,cjs,cjsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/",
    "coverage/",
    "edenx.config.ts"
  ],
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@edenx/module-tools": "1.49.2",
    "@byted/eslint-config-standard": "^3.1.1",
    "@byted/eslint-config-standard-react": "^2.1.1",
    "eslint-plugin-prettier": "~4.2.1",
    "typescript": "~5.0.4",
    "@byted/eslint-config-standard-ts": "^3.1.1",
    "eslint": "^8.57.0",
    "lint-staged": "~13.1.0",
    "prettier": "~2.8.1",
    "husky": "~8.0.1",
    "rimraf": "~3.0.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://bnpm.byted.org/"
  }
}