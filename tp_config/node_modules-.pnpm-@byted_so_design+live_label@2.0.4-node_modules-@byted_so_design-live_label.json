  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "@byted-lina/morphling-styles": "latest"
  },
  "devDependencies": {
    "@byted-lina/cli": "latest",
    "@byted-lina/component-build": "latest",
    "@byted-lina/crystal-scripts": "latest",
    "@byted-lina/lepus-utils": "latest",
    "@byted-lina/lina-config-douyin-search": "latest",
    "@byted-lina/morphling-bridge": "latest",
    "@byted-lina/morphling-icons": "latest",
    "@byted-lina/morphling-types": "latest",
    "@byted-lina/runtime": "latest",
    "@byted-lina/shared-configs": "latest",
    "@byted-lina/types": "latest",
    "@byted-lina/utils": "latest",
    "@byted-lynx/testing": "alpha",
    "@byted-lynx/testing-utils": "^0.2.2",
    "@byted-search-doc/delight-doc-packer": "latest",
    "@delight-design/dux-material-cli": "0.1.19",
    "@delight-design/dux-material-mdx": "^0.1.20",
    "@ies/aweme-color-theme": "^0.1.21",
    "@types/jest": "^27.0.1",
    "@types/node": "^18.11.17",
    "jest": "^27.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-jest": "^27.1.2",
    "tslib": "^2.4.0",
    "typescript": "^4.5.2"
  },
  "preview-url": "https://lf3-static.bytednsdoc.com/obj/eden-cn/ubojnupfuhpi/livetag.png",
  "scripts": {
    "build": "exit 0",
    "build:demo": "lina build --demo",
    "build:doc": "dux-material build:doc --type ttml-lynx",
    "build:h5": "CUSTOM_BUILD_DSL=Lynx2web lina build -o ./dist/web",
    "dev:doc": "dux-material dev:doc --type ttml-lynx",
    "prepublish:only": "lcb build -s src/components -o dist/components && npm run build:h5",
    "start": "lina start --demo",
    "start:h5": "CUSTOM_BUILD_DSL=Lynx2web lina start",
    "sync:doc": "dux-material sync --skip",
    "test": "npm run build:demo",
    "test:material": "npm run build:h5 && mv dist/web/materialInfo.json demos/test/material.json",
    "test:type": "tsc --noemit",
    "test:update": "npm run build:demo"
  }
}