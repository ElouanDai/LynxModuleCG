{
  "name": "@ies/eden-monorepo",
  "version": "3.5.0",
  "description": "EMO——Fast, Scalable, Friendly Monorepo Solution",
  "keywords": [
    "eden",
    "monorepo"
  ],
  "repository": {
    "type": "git",
    "url": "https://code.byted.org/web-solutions/emo/tree/master/packages/emo"
  },
  "author": "wumingliang",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "require": "./lib/index.js"
    },
    "./dist/*.js": {
      "types": "./dist/esm/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    },
    "./dist/*": {
      "types": "./dist/esm/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    },
    "./bin/*": "./bin/*",
    "./boilerplate": "./boilerplate/eden.monorepo.json",
    "./package.json": "./package.json"
  },
  "types": "./dist/esm/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/esm/index.d.ts"
      ],
      "dist/*.js": [
        "./dist/esm/*.d.ts"
      ],
      "dist/*": [
        "./dist/esm/*.d.ts"
      ]
    }
  },
  "bin": {
    "eden-mono": "bin/cli.js",
    "eden-monorepo": "bin/cli.js",
    "emo": "bin/cli.js",
    "emox": "bin/emox.js"
  },
  "files": [
    "bin",
    "lib",
    "dist",
    "boilerplate",
    "boilerplate/infra/.npmrc",
    "boilerplate/.gitignore"
  ],
  "dependencies": {
    "typescript": "5.0.4",
    "@byted/eden-pipeline": "2.0.1",
    "@byted/emo-native-files-hash": "0.1.1"
  },
  "devDependencies": {
    "@byted-service/tos": "^4.0.0",
    "@changesets/assemble-release-plan": "5.1.0",
    "@changesets/config": "^1.7.0",
    "@changesets/errors": "^0.1.4",
    "@changesets/read": "^0.4.7",
    "@cloudflare/json-schema-walker": "^0.1.1",
    "@edenx/monitor": "1.25.0",
    "@ies/eden-old-cache": "npm:@ies/eden-cache@0.1.32",
    "@pnpm/lockfile-file": "8.1.1",
    "@pnpm/logger": "5.0.0",
    "@pnpm/matcher": "5.0.0",
    "@rushstack/package-deps-hash": "4.0.10",
    "@seriouscoderone/toposort": "2.1.0",
    "@swc/core": "^1.3.55",
    "@swc/jest": "^0.2.23",
    "@types/anymatch": "^1.3.1",
    "@types/command-line-args": "^5.0.0",
    "@types/debug": "^4.1.8",
    "@types/fast-levenshtein": "^0.0.2",
    "@types/fs-extra": "^11.0.0",
    "@types/glob": "^7.2.0",
    "@types/ini": "^1.3.31",
    "@types/jest": "^29.2.3",
    "@types/json5": "0.0.30",
    "@types/lodash": "^4.14.195",
    "@types/micromatch": "^4.0.1",
    "@types/node": "^17.0.29",
    "@types/node-fetch": "^2.5.7",
    "@types/semver": "^7.3.13",
    "@types/tapable": "^2.2.2",
    "@types/toposort": "^2.0.3",
    "@types/yargs": "^15.0.12",
    "@types/yarnpkg__lockfile": "^1.1.4",
    "@web-doctor/code-scan": "1.3.3",
    "@web-doctor/types": "1.3.3",
    "@yarnpkg/lockfile": "^1.1.0",
    "anymatch": "^3.1.1",
    "chalk": "^2.4.2",
    "debug": "^4.3.4",
    "enhanced-resolve": "^5.14.0",
    "enhanced-tsconfig-paths-webpack-plugin": "^0.2.3",
    "enquirer": "^2.3.6",
    "exec-sh": "^0.4.0",
    "execa": "^5.1.1",
    "external-editor": "^3.1.0",
    "fast-levenshtein": "^3.0.0",
    "fixturez": "^1.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^7.1.6",
    "identity-obj-proxy": "^3.0.0",
    "ini": "^2.0.0",
    "is-subdir": "^1.2.0",
    "jest": "^29.5.0",
    "jiti": "1.21.0",
    "just-standard-deviation": "^2.2.0",
    "lodash": "^4.17.21",
    "memfs": "^3.2.0",
    "micromatch": "^4.0.2",
    "node-fetch": "^2.6.1",
    "outdent": "^0.5.0",
    "p-limit": "^3.1.0",
    "semver": "^7.5.4",
    "shift-left": "^0.1.5",
    "simple-git": "^3.17.0",
    "spawndamnit": "2.0.0",
