    "tapable": "^2.0.0",
    "term-size": "2.1.0",
    "toposort": "^2.0.2",
    "ts-json-schema-generator": "^1.0.0",
    "tsconfig-paths": "4.2.0",
    "tty-table": "^2.8.10",
    "walkdir": "^0.4.1",
    "webpack-bundle-analyzer": "4.7.0",
    "yaml": "^1.10.2",
    "yargs": "^15.3.1",
    "@byted/eden-changesets-apply-release-plan": "6.1.2",
    "@byted/eden-mono-config": "3.5.0",
    "@byted/eden-mono-shared": "3.5.0",
    "@byted/eden-mono-types": "1.1.2",
    "@byted/eden-recover-deps": "1.4.0",
    "@byted/emo-cloud-utils": "0.1.4",
    "@byted/emo-test-utils": "0.0.1",
    "@byted/ncc": "0.0.1",
    "@ies/eden-monorepo": "3.5.0",
    "@ies/emo-cache": "1.1.4"
  },
  "scripts": {
    "analyze": "npm run ncc:stats && node scripts/analyze.js",
    "bench": "rm -rf tests/.emo-test-temp && jest --verbose -c ./jest.bench.config.js && node scripts/bench.js",
    "build": "rm -rf dist lib stats && npm run build:cjs && npm run build:esm && rm -rf dist/*.tsbuildinfo && npm run ncc && npm run build:cli-entry && npm run build:schema",
    "build:cjs": "tsc --project tsconfig.build-cjs.json",
    "build:cli-entry": "ncc build dist/esm/cli-entry.js -t -o dist -e typescript -e @ies/eden-monorepo/package.json -e @byted/emo-native-files-hash && mv ./dist/index.js ./dist/cli-entry.js",
    "build:esm": "tsc --project tsconfig.build-esm.json",
    "build:schema": "node scripts/schema.js",
    "ncc": "ncc build dist/esm/index.js -t -o lib -e typescript -e @ies/eden-init/bin/bootstrap.js -e @ies/eden-monorepo/package.json -e @byted/emo-native-files-hash",
    "ncc:stats": "rm -rf lib stats && mkdir stats && npm run ncc -- --stats-out stats/stats.json",
    "start": "rm -rf dist/cjs dist/*.tsbuildinfo && npm run build:cjs -- -w",
    "test": "rm -rf tests/.emo-test-temp && jest --verbose",
    "test:watch": "NODE_ENV=test jest --watch --no-coverage --testTimeout 150000"
  }
}