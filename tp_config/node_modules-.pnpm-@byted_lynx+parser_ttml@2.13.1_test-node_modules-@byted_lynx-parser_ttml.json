{
  "name": "@byted-lynx/parser-ttml",
  "version": "2.13.1-test",
  "description": "ttml AST builder",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "serial": true
  },
  "dependencies": {
    "eslint-visitor-keys": "^2.1.0",
    "source-map": "0.7.4",
    "tslib": "2.2.0",
    "@byted-lynx/parser-syntax": "2.3.0",
    "@byted-lynx/parser-utils": "1.5.0"
  },
  "devDependencies": {
    "@types/eslint-visitor-keys": "^1.0.0",
    "@types/fs-extra": "9.0.11",
    "@types/node": "14.14.39",
    "ava": "3.15.0",
    "fs-extra": "9.1.0",
    "glob": "^10.2.2",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "typescript": "5.1.3",
    "yaml": "1.10.2"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "pnpm run clear && pnpm run build:cjs && pnpm run build:esm",
    "build:cjs": "tsc -p ./tsconfig.json --outDir ./dist/cjs -m CommonJS",
    "build:dep": "pnpm --filter @byted-lynx/parser-ttml^... run build",
    "build:esm": "tsc -p ./tsconfig.json --outDir ./dist/esm -m ESNext",
    "clear": "rimraf ./dist",
    "dev": "tsc --watch",
    "lint": "eslint --quiet --config .eslintrc.js src/**",
    "lint:fix": "eslint --quiet --fix --config .eslintrc.js src/**",
    "test": "ava --verbose --no-cache",
    "test:update": "ava --update-snapshots"
  }
}