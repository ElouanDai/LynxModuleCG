    "@types/jest": "^27.0.1",
    "@types/node": "^18.11.17",
    "execa": "^5.1.1",
    "jest": "^27.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "2.2.19",
    "ts-jest": "^27.1.2",
    "tslib": "^2.4.0",
    "typescript": "^4.5.2"
  },
