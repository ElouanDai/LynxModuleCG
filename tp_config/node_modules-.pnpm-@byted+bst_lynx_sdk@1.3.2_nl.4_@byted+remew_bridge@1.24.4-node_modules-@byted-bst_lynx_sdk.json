{
  "name": "@byted/bst-lynx-sdk",
  "version": "1.3.2-nl.4",
  "description": "Native&Lynx 混合场景 Lynx 端 BST SDK",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./plugins": "./dist/cjs/plugins/index.js",
    "./lepus": "./dist/esm/lepus/index.js"
  },
  "keywords": [
    "bst"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@byted-lynx/lynx-speedy": "^2.28.0",
    "@byted-lynx/typings": "^1.0.17",
    "@byted/remew-bridge": "^1.18.0",
    "@types/babel__core": "^7.1.20",
    "@types/lodash-es": "^4.17.6",
    "@types/tapable": "^2.2.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@babel/core": "^7.20.7",
    "@babel/types": "^7.21.2",
    "@byted-lina/runtime-ng": "^1.0.2",
    "@byted-lina/lina-douyin-search-mixins": "^1.0.12",
    "@byted-lynx/parser-ttml": "^2.6.0",
    "lodash-es": "^4"
  },
  "peerDependencies": {
    "@byted/remew-bridge": "^1.21.4"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "sh scripts/build.sh",
    "build:cjs": "tsc --module commonjs --outDir ./dist/cjs",
    "build:esm": "tsc --outDir ./dist/esm",
    "dev": "rm -rf dist & npm run dev:cjs & npm run dev:esm",
    "dev:cjs": "tsc -w --module commonjs --outDir ./dist/cjs",
    "dev:esm": "tsc -w --outDir ./dist/esm"
  }
}