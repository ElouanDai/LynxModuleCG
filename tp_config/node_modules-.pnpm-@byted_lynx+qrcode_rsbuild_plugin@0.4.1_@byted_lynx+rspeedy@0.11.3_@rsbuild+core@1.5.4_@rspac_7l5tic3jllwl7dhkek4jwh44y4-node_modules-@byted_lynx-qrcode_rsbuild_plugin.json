    "directory": "plugins/rspeedy/plugin-qrcode"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Qingyu Wang",
    "email": "wangqingyu.c0l1n@bytedance.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
