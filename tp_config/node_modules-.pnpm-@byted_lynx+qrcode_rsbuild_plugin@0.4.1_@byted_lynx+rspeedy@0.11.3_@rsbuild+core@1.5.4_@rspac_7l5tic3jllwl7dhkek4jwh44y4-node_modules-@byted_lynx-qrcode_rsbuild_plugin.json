  },
  "peerDependencies": {
    "@byted-lynx/rspeedy": "^0.7.0 || ^0.8.0 || ^0.9.0 || ^0.10.0"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "api-extractor": "api-extractor run --verbose",
    "build": "rslib build",
    "docs:update": "api-extractor run --verbose --local"
