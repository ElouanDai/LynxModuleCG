    "@byted-lina/morphling-bridge": "latest",
    "@byted-lina/morphling-icons": "latest",
    "@byted-lina/morphling-types": "latest",
    "@byted-lina/runtime": "latest",
    "@byted-lina/shared-configs": "latest",
