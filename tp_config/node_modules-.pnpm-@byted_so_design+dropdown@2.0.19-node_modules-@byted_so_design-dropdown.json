    "@byted-lina/utils": "latest",
    "@byted-lynx/lepus-runtime": "0.10.4",
    "@byted-lynx/lynx-speedy": "^3.5.4",
    "@byted-lynx/plugin-miniapp-nodiff": "0.10.9",
    "@byted-lynx/plugin-tailwindcss": "^2.4.2",
    "@byted-lynx/speedy-config-preset-env": "^1.6.0",
    "@byted-lynx/testing": "^2.12.0-release.ffb7c42a",
