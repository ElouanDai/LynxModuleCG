{
  "name": "@byted-lina/tracker",
  "version": "2.9.2",
  "description": "lina tracker",
  "keywords": [
    "lina",
    "tracker"
  ],
  "license": "ISC",
  "maintainers": [
    "yanhongwei",
    "zhaohaibo.finger"
  ],
  "sideEffects": [
    "./src/tracker/index.ts"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist &&  npm run build:esm && npm run build:cjs && npm run build:declaration",
    "build:cjs": "tsc --outDir dist/cjs --module CommonJS",
    "build:declaration": "tsc --target ESNext --emitDeclarationOnly --declaration --declarationDir dist/types",
    "build:esm": "tsc --outDir dist/esm --module ESNext",
    "start": "tsc -w",
    "test": "jest",
    "test:type": "tsc --noemit"
  },
  "dependencies": {
    "@byted-lina/morphling-bridge": "workspace:*",
    "@byted-lina/morphling-types": "workspace:*"
  },
  "devDependencies": {
    "@byted-lina/shared-configs": "workspace:*",
    "@byted-lina/types": "workspace:*",
    "@types/jest": "^27.0.1",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.2"
  }
}