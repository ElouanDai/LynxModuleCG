{
  "name": "@byted-lynx/lepus-runtime",
  "version": "3.8.4",
  "description": "default mode lepus runtime",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./jsRender": "./lib/jsRender/index.js",
    "./mainThread": "./lib/mainThread/index.js",
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "jsRender": [
        "./lib/jsRender/index.d.js"
      ]
    }
  },
  "files": [
    "dist/ttml-runtime.template.js",
    "lib",
    "CHANGELOG.md",
    "package.json",
    "README.md",
    "!lib/**/*.map"
  ],
  "dependencies": {
    "@byted-lynx/worklet-common": "0.0.5",
    "@byted-lynx/ttmx-runtime-shared": "0.3.4"
  },
  "devDependencies": {
    "@byted-lynx/type-lynx": "3.4.11",
    "@rslib/core": "0.15.0"
  },
  "peerDependencies": {
    "@byted-lynx/type-lynx": "3.4.11"
  },
  "scripts": {
    "build": "node build.mjs",
    "build:tsc": "tsc --project tsconfig.build.json",
    "test": "vitest"
  }
}