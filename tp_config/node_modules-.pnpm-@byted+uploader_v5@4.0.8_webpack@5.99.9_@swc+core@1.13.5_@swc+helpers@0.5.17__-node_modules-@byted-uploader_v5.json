  "files": [
    "lib",
    "index.js"
  ],
  "scripts": {
    "debug:example": "NODE_ENV=dev CUS_ENV=BOE ROUTE_IP=10.225.130.44 CONSUL_HTTP_HOST=10.225.130.44 ts-node ./examples/multipart_sample.ts",
    "build": "rm -rf lib && tsc",
    "build:watch": "rm -rf lib && tsc -w",
    "lint": "eslint . --ext ts,js --fix",
    "lint-check": "eslint . --ext ts,js",
    "test": "jest --config ./jest.config.json --coverage --env=node",
    "commit": "commit",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog-all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "patch": "./scripts/release.sh -r patch",
    "minor": "./scripts/release.sh -r minor",
    "major": "./scripts/release.sh -r major",
    "replace": "replace 'https://code.byted.org///' 'https://code.byted.org/toutiao-fe/uploader_v5_nodejs/' CHANGELOG.md",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@byted-lint/eslint-plugin-meta": "^2.3.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/config-lerna-scopes": "^8.0.0",
    "@commitlint/prompt-cli": "^8.2.0",
    "@types/jest": "^24.0.13",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^12.0.5",
    "@types/node-fetch": "^2.3.4",
    "@types/query-string": "^6.3.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.28",
    "eslint": "^8.54.0",
    "husky": "^2.4.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "lerna": "^3.14.1",
    "lint-staged": "^9.4.3",
    "prettier": "^1.17.1",
    "replace": "^1.1.1",
    "standard-version": "^7.0.1",
    "ts-jest": "^24.0.2",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@byted-service/env": "^1.15.1",
    "@byted-service/fetch": "^2.6.5",
    "@byted-service/tos": "4.1.0",
    "@byted/aws-signers-v4": "^0.0.6",
    "@byted/node-polaris": "1.2.59",
    "debug": "^4.1.1",
    "lodash.get": "^4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier",
      "eslint --fix",
      "git add"
    ]
  }
