{
  "name": "@byted-lina/runtime-ng",
  "version": "1.6.2",
  "description": "lina runtime ng",
  "keywords": [
    "lina",
    "tracker"
  ],
  "license": "ISC",
  "maintainers": [
    "zhaohaibo.finger"
  ],
  "sideEffects": [
    "./dist/esm/index.js"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "build:cjs": "ttsc --outDir dist/cjs --module CommonJS",
    "build:esm": "ttsc --outDir dist/esm --module ESNext",
    "start": "rm -rf dist && npm run start:cjs & npm run start:esm",
    "start:cjs": "ttsc -w --outDir dist/cjs --module CommonJS",
    "start:esm": "ttsc -w --outDir dist/esm --module ESNext"
  },
  "dependencies": {
    "ts-toolbelt": "^9.6.0"
  },
  "devDependencies": {
    "@byted-lina/shared-configs": "workspace:*",
    "@byted-lina/types": "workspace:*",
    "@byted-lynx/type-ttml": "0.0.3",
    "@types/jest": "^27.0.1",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "ttypescript": "^1.5.15",
    "typescript": "^4.5.2"
  }
}