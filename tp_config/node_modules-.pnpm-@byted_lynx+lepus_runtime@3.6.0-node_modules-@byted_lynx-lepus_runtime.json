{
  "name": "@byted-lynx/lepus-runtime",
  "version": "3.6.0",
  "description": "default mode lepus runtime",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./jsRender": "./lib/jsRender/index.js",
    "./mainThread": "./lib/mainThread/index.js",
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "jsRender": [
        "./lib/jsRender/index.d.js"
      ]
    }
  },
  "files": [
    "lib",
    "CHANGELOG.md",
    "package.json",
    "README.md",
    "!lib/**/*.map"
  ],
  "dependencies": {
    "@byted-lynx/ttmx-runtime-shared": "0.0.2",
    "@byted-lynx/worklet-common": "0.0.5"
  },
  "devDependencies": {
    "@byted-lynx/tasm": "4.4.8",
    "@lynx-js/runtime-wrapper-webpack-plugin": "0.1.2"
  },
  "scripts": {
    "build:tsc": "tsc --project tsconfig.build.json",
    "prebundle": "rm -rf dist",
    "bundle": "rslib build -c ./configs/rslib.background.config.js & rslib build -c ./configs/rslib.main-thread.config.js",
    "postbundle": "node encode.mjs",
    "bundle:dev": "DEV=true pnpm bundle",
    "test": "vitest"
  }
}