{
  "name": "@byted-lina/docusaurus-plugin-lynx-docgen",
  "version": "1.0.4",
  "maintainers": [
    "linyuhan.root"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch --no-dts",
    "test:type": "tsc --noemit"
  },
  "dependencies": {
    "@byted-lina/extract-lina-component-info": "workspace:*",
    "fast-glob": "^3.2.11"
  },
  "devDependencies": {
    "@byted-lina/shared-configs": "workspace:*",
    "@docusaurus/types": "^2.0.1",
    "@types/node": "*",
    "tsup": "^5.11.13",
    "typescript": "^4"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "target": "node14",
    "format": [
      "cjs"
    ],
    "sourcemap": true,
    "clean": true
  }
}