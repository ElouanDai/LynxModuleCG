{
  "name": "@byted-lina/morphling-bridge",
  "version": "7.0.4",
  "description": "morphling JSBridge",
  "keywords": [
    "morphling",
    "JSBridge",
    "search_hybrid"
  ],
  "license": "ISC",
  "maintainers": [
    "gouwantong",
    "chenjiawei.cjw",
    "zhusichao"
  ],
  "sideEffects": true,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist &&  npm run build:esm && npm run build:cjs && npm run build:declaration",
    "build:cjs": "tsc --outDir dist/cjs --module CommonJS",
    "build:declaration": "tsc --target ESNext --emitDeclarationOnly --declaration --declarationDir dist/types",
    "build:esm": "tsc --outDir dist/esm --module ESNext",
    "start": "tsc -w",
    "test": "jest",
    "test:type": "tsc --noemit"
  },
  "dependencies": {
    "@bridge/search_hybrid": "3.1.16"
  },
  "devDependencies": {
    "@byted-lina/shared-configs": "workspace:*",
    "@byted-lina/types": "workspace:*",
    "@types/jest": "^27.0.1",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.2"
  }
}