{
  "name": "@fe/aweme-jsb-utils",
  "version": "0.0.5",
  "license": "ISC",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "tiger test",
    "release": "tiger release",
    "release:big": "tiger release --versionType big",
    "release:feat": "tiger release --versionType feat",
    "release:fix": "tiger release --versionType fix",
    "release:alpha": "tiger release --versionType alpha",
    "doc:dev": "tiger doc",
    "doc:build": "tiger doc --build",
    "doc:deploy": "tiger doc --publish"
  },
  "repository": {},
  "files": [
    "dist",
    "types"
  ],
  "engines": {
    "node": ">=12.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tiger test && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs,mjsx,cjs,cjsx}": [
      "tiger lint",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@jupiter-app"
    ],
    "rules": {
      "@typescript-eslint/no-base-to-string": "off"
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/",
    "types",
    "cli.conf.js",
    "*.test.ts"
  ],
  "peerDependencies": {},
  "dependencies": {
    "@babel/runtime": "^7.11.2"
  },
  "devDependencies": {
    "@fe/tiger": "latest"
  },
  "author": {
    "name": "gaopeng.anran",
    "email": "gaopeng.anran@bytedance.com",
    "url": "lark://applink.feishu.cn/client/chat/open?openId=ou_3013551edc400dfac9db7c089fae7091"
  }
}