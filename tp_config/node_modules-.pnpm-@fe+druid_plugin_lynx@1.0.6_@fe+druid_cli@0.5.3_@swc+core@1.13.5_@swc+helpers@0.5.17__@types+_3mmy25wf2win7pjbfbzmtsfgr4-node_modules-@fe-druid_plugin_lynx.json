{
  "name": "@fe/druid-plugin-lynx",
  "version": "1.0.6",
  "description": "搜索lynx ssr卡片支持",
  "license": "MIT",
  "author": "weurkang",
  "main": "dist/index.js",
  "module": "dist/druid-plugin-lynx.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/druid-plugin-lynx.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/druid-plugin-lynx.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@tsconfig/recommended": "^1.0.1",
    "dts-cli": "^1.5.1",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@fe/byted-workflow": "^0.4.4",
    "glob": "^7.2.0",
    "http-proxy-middleware": "^2.0.6",
    "qrcode": "^1.5.0",
    "qs": "^6.10.1",
    "@fe/druid-libs-core": "0.5.8",
    "@fe/druid-libs-dev": "0.7.11",
    "@fe/druid-libs-utils": "1.0.2",
    "@fe/search_offline_utils": "0.2.0"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build --target=node --format=cjs",
    "lint": "dts lint",
    "size": "size-limit",
    "start": "dts watch --target=node --format=cjs"
  }
}