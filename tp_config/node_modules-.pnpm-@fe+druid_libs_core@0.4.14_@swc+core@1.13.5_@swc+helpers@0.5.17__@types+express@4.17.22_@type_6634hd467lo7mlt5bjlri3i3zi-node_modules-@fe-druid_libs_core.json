{
  "name": "@fe/druid-libs-core",
  "version": "0.4.14",
  "license": "MIT",
  "author": "wuerkang",
  "main": "dist/index.js",
  "module": "dist/druid-libs-core.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/druid-libs-core.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/druid-libs-core.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@tsconfig/recommended": "^1.0.1",
    "dts-cli": "^1.5.2",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "http-proxy-middleware": "^2.0.6",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "validate": "^5.2.0",
    "webpack": "^5.73.0",
    "@fe/druid-cli": "0.5.1",
    "@fe/search_offline_utils": "0.2.0"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build --target=node --format=cjs",
    "lint": "dts lint",
    "size": "size-limit",
    "start": "dts watch"
  }
}