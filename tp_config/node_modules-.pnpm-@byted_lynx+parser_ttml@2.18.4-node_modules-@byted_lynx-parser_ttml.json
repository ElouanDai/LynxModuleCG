{
  "name": "@byted-lynx/parser-ttml",
  "version": "2.18.4",
  "description": "ttml AST builder",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./cjs/index.d.ts",
        "default": "./cjs/index.js"
      }
    }
  },
  "source": "./src/index.ts",
  "types": "./esm/index.d.ts",
  "files": [
    "cjs",
    "!cjs/**/*.js.map",
    "esm",
    "!esm/**/*.js.map",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "dependencies": {
    "@byted-lynx/parser-syntax": "2.3.6",
    "source-map": "^0.7.4",
    "@byted-lynx/parser-utils": "1.5.4"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.4",
    "@types/node": "^22.15.30",
    "glob": "^11.0.2",
    "rimraf": "^6.0.1",
    "yaml": "2.8.0"
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "scripts": {
    "test": "vitest",
    "test:update": "vitest -u"
  }
}