{
  "name": "@byted-lina/lina-mixin-idle-preload",
  "version": "3.0.79",
  "description": "lina-mixin-idle-preload",
  "keywords": [
    "lina",
    "mixin",
    "idle-preload"
  ],
  "license": "ISC",
  "maintainers": [
    "yanhongwei",
    "zhengxu.foround"
  ],
  "sideEffects": [
    "./dist/esm/index.js"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "build:cjs": "ttsc --outDir dist/cjs --module CommonJS",
    "build:esm": "ttsc --outDir dist/esm --module ESNext",
    "start": "rm -rf dist && npm run start:cjs & npm run start:esm",
    "start:cjs": "ttsc -w --outDir dist/cjs --module CommonJS",
    "start:esm": "ttsc -w --outDir dist/esm --module ESNext"
  },
  "dependencies": {
    "@byted-lina/lina-mixin-idle-task": "workspace:*",
    "@byted-lina/morphling-bridge": "workspace:*",
    "@byted-lina/morphling-types": "workspace:*",
    "@byted-lina/runtime-ng": "workspace:*",
    "@byted-lina/utils": "workspace:*"
  },
  "devDependencies": {
    "@byted-lina/shared-configs": "workspace:*",
    "@types/jest": "^27.0.1",
    "@types/node": "^18.0.6",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "ttypescript": "^1.5.15",
    "typescript": "^4.5.2"
  }
}