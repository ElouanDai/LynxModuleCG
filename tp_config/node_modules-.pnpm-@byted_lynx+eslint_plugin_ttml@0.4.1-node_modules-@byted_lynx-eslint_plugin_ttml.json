{
  "name": "@byted-lynx/eslint-plugin-ttml",
  "version": "0.4.1",
  "description": "An eslint plugin for TTML",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "license": "ISC",
  "exports": {
    ".": "./dist/index.js",
    "./ttml": "./dist/ttml.js",
    "./json": "./dist/json.js",
    "./typescript": "./dist/typescript.js",
    "./commonMode": "./dist/commonMode.js",
    "./strictMode": "./dist/strictMode.js",
    "./sfc": "./dist/sfc.js"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json",
    "CHANGELOG.md",
    "docs"
  ],
  "dependencies": {
    "@byted-lynx/parser-ttml-eslint": "0.2.5",
    "jsonc-eslint-parser": "2.4.0"
  },
  "devDependencies": {
    "esbuild-register": "3.4.2",
    "eslint": ">=7.0.0",
    "eslint-doc-generator": "1.7.0",
    "eslint-plugin-eslint-plugin": "5.4.1",
    "eslint-plugin-node": "11.1.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json -m CommonJS",
    "dev": "npm run build -- --watch",
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "test": "npm run build && mocha './tests/**/*.ts'",
    "update:eslint-docs": "npm run build && eslint-doc-generator"
  }
}