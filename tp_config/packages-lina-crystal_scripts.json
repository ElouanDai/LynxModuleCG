{
  "name": "@byted-lina/crystal-scripts",
  "version": "1.0.6",
  "description": "Crystal 组件开发套件",
  "maintainers": [
    "linyuhan.root"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch --no-dts",
    "test": "jest",
    "test:type": "tsc --noemit"
  },
  "dependencies": {
    "@byted-lina/extract-lina-component-info": "workspace:*",
    "lodash": "^4",
    "ts-morph": "^14.0.0"
  },
  "devDependencies": {
    "@byted-lina/crystal-types": "workspace:*",
    "@byted-lina/shared-configs": "workspace:*",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "*",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "tsup": "^5.11.13",
    "typescript": "^4.5.2"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "target": "node14",
    "sourcemap": true,
    "clean": true
  }
}