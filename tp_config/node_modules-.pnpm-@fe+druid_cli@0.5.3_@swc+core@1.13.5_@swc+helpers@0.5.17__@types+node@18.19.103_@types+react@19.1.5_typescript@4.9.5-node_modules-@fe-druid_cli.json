{
  "name": "@fe/druid-cli",
  "version": "0.5.3",
  "description": "搜索前端团队出品，本地开发脚手架，灵活、交互友好、支持插件化",
  "author": "shenhan.sh",
  "bin": {
    "druid": "./bin/run.js"
  },
  "homepage": "https://github.com/bytedance/druid-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "bytedance/druid-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "2.7.1",
    "@oclif/plugin-help": "5.2.8",
    "@oclif/plugin-plugins": "2.4.3",
    "@types/lodash": "^4.14.184",
    "axios": "0.27.2",
    "compare-versions": "^5.0.1",
    "findup-sync": "5.0.0",
    "ink": "3.2.0",
    "ink-multi-select": "2.0.0",
    "ink-select-input": "4.2.1",
    "ink-spinner": "4.0.3",
    "lodash": "^4.17.21",
    "ps-tree": "^1.2.0",
    "react": "17.0.2",
    "resolve": "1.22.0",
    "signale": "1.4.0",
    "tree-kill": "^1.2.2",
    "v8-compile-cache": "^2.3.0",
    "@fe/druid_tea": "0.4.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.13",
    "@types/chai": "^4",
    "@types/findup-sync": "^4.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/react": "17.0.30",
    "@types/resolve": "^1.20.2",
    "@types/signale": "^1.4.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "3.7.2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.9.3"
  },
  "oclif": {
    "bin": "druid",
    "dirname": "druid",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/bytedance/druid-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "shx rm -rf dist && tsc -b -w",
    "start": "shx rm -rf dist && tsc -b -w",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "version": "oclif readme && git add README.md"
  }
}