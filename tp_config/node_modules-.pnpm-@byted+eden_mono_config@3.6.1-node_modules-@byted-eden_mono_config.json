  "dependencies": {
    "ajv": "^8.12.0",
    "better-ajv-errors": "^1.2.0",
    "debug": "^4.3.4",
    "globby": "^11",
    "lodash": "^4.17.21",
    "semver": "^7.5.4",
    "yaml": "^1.10.2",
    "@byted/eden-mono-shared": "3.6.1",
    "@byted/eden-mono-types": "1.2.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.55",
    "@swc/jest": "^0.2.23",
    "@types/debug": "^4.1.8",
    "@types/glob": "^7.2.0",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.195",
    "@types/node": "^17.0.29",
    "@types/semver": "^7.3.13",
    "fixturez": "^1.1.0",
