    "@byted/eden-mono-config": "3.6.1"
  },
  "scripts": {
    "build": "rm -rf dist && npm run build:schema && npm run build:cjs && npm run build:esm && rm -rf dist/*.tsbuildinfo",
    "build:cjs": "tsc --project tsconfig.build-cjs.json",
    "build:esm": "tsc --project tsconfig.build-esm.json",
    "build:schema": "node ./scripts/schema.js && prettier 'src/parser/**' --write",
    "start": "npm run build && npm run build:cjs -- -w",
    "test": "rm -rf tests/.emo-test-temp && jest"
  }
