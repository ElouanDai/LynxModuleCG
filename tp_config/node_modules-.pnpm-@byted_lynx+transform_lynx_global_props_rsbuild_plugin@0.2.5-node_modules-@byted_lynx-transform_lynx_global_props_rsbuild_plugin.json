{
  "name": "@byted-lynx/transform-lynx-global-props-rsbuild-plugin",
  "version": "0.2.5",
  "description": "A Rsbuild plugin to transform `__globalProps` to `lynx.__globalProps` in TTML.",
  "keywords": [
    "rsbuild",
    "Lynx",
    "TTML",
    "__globalProps"
  ],
  "repository": {
    "type": "git",
    "url": "https://code.byted.org/lynx/rspeedy.git",
    "directory": "packages/ttml/plugin-transform-global-props"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Hengchang Lu",
    "email": "luhengchang@bytedance.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "!lib/**/*.js.map",
    "CHANGELOG.md",
    "README.md",
    "swc_plugin_transform_ttml_global_props.wasm"
  ],
  "devDependencies": {
    "@rsbuild/core": "1.3.21"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build:debug": "cargo build -p swc_plugin_transform_ttml_global_props --target wasm32-unknown-unknown && cp ../../../target/wasm32-unknown-unknown/debug/swc_plugin_transform_ttml_global_props.wasm .",
    "build:release": "cargo build --release -p swc_plugin_transform_ttml_global_props --target wasm32-unknown-unknown && cp ../../../target/wasm32-unknown-unknown/release/swc_plugin_transform_ttml_global_props.wasm .",
    "test": "npm run build:debug && vitest run"
  }
}